{
  "name": "prettifier",
  "version": "0.3.4",
  "description": "Setup prettier and commit hooks automatically",
  "main": "index.js",
  "author": "Netto Farah",
  "license": "MIT",
  "homepage": "http://github.com/nettofarah/prettifier",
  "repository": {
    "type": "git",
    "url": "https://github.com/nettofarah/prettifier.git"
  },
  "keywords": [
    "prettier",
    "lint-staged",
    "husky",
    "prettier config"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "fs-promise": "^2.0.3",
    "has-yarn": "^1.0.0",
    "inquirer": "^3.2.2",
    "listr": "^0.12.0",
    "lodash": "^4.17.4",
    "package-json-validator": "^0.6.2",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "bin": {
    "prettifier": "./bin/prettifier.js"
  }
}
